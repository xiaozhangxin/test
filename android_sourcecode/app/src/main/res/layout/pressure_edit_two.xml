                android:paddingRight="16dp"
                android:textColor="@color/colorTextG3"
                android:textSize="14sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/must_img_not"
                android:drawablePadding="2dp"
                android:paddingLeft="1dp"
                android:text="水工姓名"
                android:textColor="@color/colorTextG3"
                android:textSize="14sp" />

            <androidx.appcompat.widget.AppCompatAutoCompleteTextView
                android:completionThreshold="1"
                android:id="@+id/tvWEight"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:gravity="end|center_vertical"
                android:hint="请输入水工姓名"
                android:paddingRight="16dp"
                android:textColor="@color/colorTextG3"
                android:textSize="14sp" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/colorTextF3" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="水工电话"
                android:textColor="@color/colorTextG3"
                android:textSize="14sp" />

            <androidx.appcompat.widget.AppCompatAutoCompleteTextView
                android:completionThreshold="1"
                android:id="@+id/tvWNine"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:gravity="end|center_vertical"
                android:hint="请输入水工电话"
                android:inputType="number"
                android:maxLength="11"
                android:paddingRight="16dp"
                android:textColor="@color/colorTextG3"
                android:textSize="14sp" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/colorTextF3" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="水工积分"
                android:textColor="@color/colorTextG3"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/tvWTen"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:gravity="end|center_vertical"
                android:hint="请输入水工积分"
                android:inputType="number"
                android:maxLength="6"
                android:paddingRight="16dp"
                android:textColor="@color/colorTextG3"
                android:textSize="14sp" />
        </LinearLayout>


        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/colorTextF3" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:visibility="gone"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="水工累计积分"
                android:textColor="@color/colorTextG3"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/tvWEleven"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:gravity="end|center_vertical"
                android:hint="请输入水工累计积分"
                android:paddingRight="16dp"
                android:textColor="@color/colorTextG3"
                android:textSize="14sp" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/colorTextF3" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="积分卡卡号"
                android:textColor="@color/colorTextG3"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/tvWTwelve"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:gravity="end|center_vertical"
                android:hint="请输入积分卡卡号"
                android:paddingRight="16dp"
                android:textColor="@color/colorTextG3"
                android:textSize="14sp" />
        </LinearLayout>






    </LinearLayout>
</merge>
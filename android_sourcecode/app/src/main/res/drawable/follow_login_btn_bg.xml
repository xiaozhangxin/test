<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/top_bar_yq" />
            <corners android:radius="20dp" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/top_bar_yq_bg" />
            <corners android:radius="20dp" />
            <padding
                android:bottom="10dp"
                android:left="20dp"
                android:right="20dp"
                android:top="10dp" />
        </shape>
    </item>
</layer-list>